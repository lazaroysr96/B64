<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape  android:shape="rectangle">
			<solid android:color="#004060"/>
			<corners android:radius="5dp"/>
			<padding
				android:left="5dp"
				android:top="5dp"
				android:right="5dp"
				android:bottom="5dp"/>
		</shape>
	</item>
	<item >
		<shape  android:shape="rectangle">
			<solid android:color="#121e24"/>
			<corners android:radius="5dp"/>
			<padding
				android:left="5dp"
				android:top="5dp"
				android:right="5dp"
				android:bottom="5dp"/>
		</shape>
	</item>
</selector>
